  "repository": "https://github.com/JamieCleare2525/gem-checker.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "file-system": "^2.2.2",
    "jquery": "^3.0.0"
  },
  "devDependencies": {
    "electron-prebuilt": "^1.4.13"
  }
}
